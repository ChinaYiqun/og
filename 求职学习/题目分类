class Solution:
    def reverseWords(self, s: str) -> str:


        #str.split()
        #print(s.split(' '))
        #l = s.split(' ')
        # str.join()
        #print(' '.join(l))
        # print(s[::-1])
        # strip()

        # return ' '.join(s.strip().split(' ')[::-1])
        start,end =0,len(s)-1
        # 去掉首位空格
        while(s[start]== ' '):
            start += 1
        while(s[end]==' '):
            end -= 1
        s = s[start:end+1]
        # 得到list单词
        l = []
        start,end = 0,0
        while(end < len(s)):
            #扩大窗口
            while(end< len(s) and s[end]!=' '):
                end += 1
            l.append(s[start:end])
            while(end<len(s) and s[end]==' '):
                end += 1
            start = end
        res = ""          
        # 反转list
        # 连接
        end = len(l) -1
        while(end >= 0):
            # 这种写法狠不好
            res += l[end]+' '
            end -= 1
        return res[0:-1]

