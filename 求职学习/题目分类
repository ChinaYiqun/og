149. 直线上最多的点数 【也是重点】
class Solution:
    def maxPoints(self, points: List[List[int]]) -> int:
        if not points:
            return 0

        # 寻找每一个点的 斜率的共同点个数
        ans = 1
        for i in range(len(points)):
            i_dict = {'+':1}
            dup = 0
            for j in range(i+1,len(points)):
                # i, j
                if points[i] == points[j]:
                    dup += 1
                    continue
                p1x , p1y = points[i]
                p2x , p2y = points[j]
                
                k = '-'
                #计算斜率k
                try:
                    k = (p1y - p2y) / (p1x - p2x)
                except:
                    pass
                if k not in i_dict:
                    i_dict[k] = 2
                else:
                    i_dict[k] += 1
            
            ans = max(ans,max(i_dict.values())+dup)
            print(i_dict)
        return ans
